{
    "manifest_version": 3,
    "name": "Parental Control",
    "version": "1.0",
    "description": "An extension for parental control with login and register",
    "permissions": ["storage"],
    "action": {
      "default_popup": "login.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
  
